-- actor creator
function makea(x,y,s,w,h)
	local a={}
 --pixel to sprite size
	a.x=x*8
	a.y=y*8
	a.inv=-1
	a.hb={x=0,y=0,w=8,h=8}
	a.so= s --starting sprite
	a.s=s -- sprite number
	a.dr=true --to draw or not
	a.w=w or 1
	a.h=h or 1
 a.sx,a.sy=false, false
 a.draw=function(a)end
 a.update=function(a)end
 return a
end

function makep(x,y,s,w,h)
	local p=makea(x,y,s,w,h)
	p.dx=0
	p.dy=0
	p.accdx=.2
	p.maxdx=1.2
	p.fric=0.08
	p.gravity=.06
	return p
end	

--main physics handling for
--any physics following object
function p_upd(p)
 repeat
 	if p.dx>0 then
 		p.dx-=p.fric
 		if(p.dx<0)p.dx=0
 	end
 	if p.dx<0 then
 		p.dx+=p.fric
 		if(p.dx>0)p.dx=0
 	end
 until not solid (p.x+p.dx,p.y)
 p.x+=p.dx
 
 p.dy+=p.gravity
 while solid(p.x,p.y+p.dy) do
 	if p.dy>0 then
 		p.dy-=p.gravity
 			if(p.dy<0)p.dy=0
 	end
 	if p.dy<0 then
 		p.dy+=p.gravity
 			if(p.dy>0)p.dy=0
 	end
 end	
 p.y+=p.dy
end

--invulnerability
function invul(p)
	if p.inv>-1 then p.inv+=1 end
 if p.inv>30 then
  p.inv=-1
 	p.hb.x=0
 end
 if p.inv>0 and (p.inv%3)~=0 then
 	p.dr=false
 	p.hb.x=2047
 end
 if p.inv>0 and (p.inv%3)==0 then
 	p.dr=true
 	p.hb.x=2047
 end
 	
end

--movement variable handling
--for player
function p_move(p,d)
	p.dx+=p.accdx*d
	if(p.dx>p.maxdx)p.dx-=p.accdx
	if(p.dx<-p.maxdx)p.dx+=p.accdx
end

--main jump handling
--for player
function p_jump(p,d)
	d=d or 1.7
	if solid(p.x,p.y+1) and not solid(p.x,p.y-1)then
	 p.dy=-d
	end
end

--movement animations for player
--assumes two things:
--1.each actor only has 4 sprites
--2.standing 1, run 2&3, jump 4
function p_moveanim(a)
	if a.dy>0 or a.dy<0 then
	 a.s=a.so+3
	elseif a.dx>0 or a.dx<0 then
  a.s += .2
 	if a.s > a.so+3 then
 	 a.s = a.so+1
 	end
 else
 	a.s=a.so
 end
 spr(a.s,a.x,a.y,a.w,a.h,a.sx,a.sy)	
end


--uses flags for ground collision
function solid(x,y,s)
	s=s or 0
 if(fget(mget(flr(x/8),flr(y/8)),s))return true
 if(fget(mget(flr((x+7)/8),flr(y/8)),s))return true 
 if(fget(mget(flr(x/8),flr((y+7)/8)),s))return true 
 if(fget(mget(flr((x+7)/8),flr((y+7)/8)),s))return true
 if(x<0 or x>121 or y<0 or y>64)return true 
	return false
end





