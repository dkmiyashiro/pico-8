-- game handling functions

--main init
function game_init()
 mode = 2
 bosses={}
 ppro={}
 bpro={}
 rdir = true
 sun_init()
 bee=makep(3,6,0)
 bee.hb={x=1,y=0,w=5,h=8}
 bee.update=p_upd
 bee.draw=p_moveanim
 bee.weapon=0
 camera(0,-32)
end

--main update function
function game_upd()
 -- for delay in weapon use
 if (pdelaytime<pdelay and pdelaytime>0) pdelaytime+=1  
	if (pdelaytime==pdelay) pdelaytime=0
	--movement and button inputs
	if btn(0) then
		bee.sx=true
		rdir=false
	 p_move(bee,-1)
	end
	if btn(1) then
		bee.sx=false
		rdir=true
		p_move(bee,1)
	end
	if btnp(2) then
	 sfx(0)
		p_jump(bee)
	end
	if btnp(3) then
	 switchweapon()
	end
	if btnp(4) then
	 attack()
	end
	--actual updates
	
	--player update
	bee.update(bee)
	--boss updates
	for _,b in pairs(bosses) do
		b.update(b)
	end
	for _,pro in pairs(ppro) do
		pro.update(pro)
		if pro.bad then 
			collcheck(pro,bee)
		else
			for _,b in pairs(bosses) do
				collcheck(pro,b)
			end
		end
	end
end

--main draw function
function game_draw()
	cls()
	rectfill(0,0,128,64,6)
	map(0,0,0,0,16,9)
	for _,b in pairs(bosses) do
		b.draw(b)
		if (debug) hbdraw(b)
	end
	for _,pro in pairs(ppro) do
		pro.draw(pro)
  if (debug) hbdraw(pro)	
	end
	bee.draw(bee)
	if (debug) hbdraw(bee)
end

