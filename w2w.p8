--random movements for x
function mrx(actor)
	if actor.x<actor.x_min then 
  actor.x+=rnd(2)
  elseif actor.x>actor.x_max then
  actor.x-=rnd(2)
  else
  actor.x+=rnd(1)
  actor.x-=rnd(1)
  end
end

--random movements for y
function mry(actor)
	if actor.y<actor.y_min then 
  actor.y+=rnd(2)
  elseif actor.y>actor.y_max then
  actor.y-=rnd(2)
  else
  actor.y+=rnd(1)
  actor.y-=rnd(1)
  end
end
