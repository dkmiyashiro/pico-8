--projectiles and collision
pdelay=30
pdelaytime=0

function procreate(x,y,s,w,h,bad,dam)
	local pro=makep(x,y,s,w,h)
	pro.update=pro_upd
 pro.dmg=dam
 pro.bad=bad or false
 pro.dltme=false
 return add(ppro,pro)
end

function launch(p,d)
	d=d or 2
	if not solid(p.x,p.y-1)then
	 p.dy=-d
	end
end	

function attack()
 if pdelaytime==0 then
  pdelaytime+=1
  if bee.weapon==0 then tie()
  elseif bee.weapon==1 then
  elseif bee.weapon==2 then
  elseif bee.weapon==3 then
  end
 end
end

function switchweapon()
 bee.weapon+=1
 if bee.weapon>bee.weaponmax then
  bee.weapon=0
 end
end

function tie()
	local ti = procreate(bee.x/8,bee.y/8,4,1,1,false,5)
	ti.draw=pro_rot
	ti.gravity = 0 
	ti.fric = 0.008
	if(debug)ti.dmg=100
	--launch(ti)
	if(rdir)p_move(ti,6)
	if(not rdir)p_move(ti,-6)
end 

function pro_upd(pro)
	p_upd(pro)
	if (pro.dx==0 and pro.dy==0) or pro.dltme then
	 del(ppro,pro)
		sfx(1)
	end
end

--testing collision with hitboxes
function collcheck(p,a)
	if a.x+a.hb.x+a.hb.w>p.x+p.hb.x and
			a.y+a.hb.y+a.hb.h>p.y+p.hb.y and
			a.x+a.hb.x<p.x+p.hb.x+p.hb.w and
			a.y+a.hb.y<p.y+p.hb.y+p.hb.h
		then
			dmgcalc(p,a)
			a.inv=0
		 p.dltme=true
		 sfx(3)
			return true
	end
	return false
end

--damage calculation
function dmgcalc(p,a)
	a.hp-=p.dmg
end

--rotation for tie
function pro_rot(pro)
	pro.s+=.2
	if pro.s>pro.so+2 then
		if not pro.sy and not pro.sx then
		 pro.sy=true
		 pro.s=pro.so
		else
			pro.sy=false
			pro.sx=false
			pro.s=pro.so
		end
	elseif pro.sy then
		pro.sy=false
		pro.sx=true
	end
	spr(pro.s,pro.x,pro.y,pro.w,pro.h,pro.sx,pro.sy)
end
